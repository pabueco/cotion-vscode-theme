{
  "name": "cotion-theme - Dark",
  "type": "dark",
  "colors": {
    "terminal.ansiGreen": "#4CB782",
    "terminal.ansiBrightGreen": "#a2d484",
    "terminal.ansiMagenta": "#cba0ff",
    "terminal.ansiYellow": "#ffcc6d",
    "terminal.ansiCyan": "#26B5CE",
    "terminal.ansiRed": "#eb5757",
    "foreground": "#c3c3c3",
    "icon.foreground": "#c3c3c3",
    "focusBorder": "#2eaadc",
    "contrastBorder": "#3f3f3f",
    "errorForeground": "#eb5757",
    "selection.background": "#2eaadc",
    "button.background": "#2eaadc",
    "button.hoverBackground": "#42bef0",
    "progressBar.background": "#42bef0",
    "input.background": "#343434",
    "input.border": "#494949",
    "inputOption.activeBackground": "#2eaadc",
    "inputValidation.errorBorder": "#eb5757",
    "editor.background": "#202020",
    "editor.foreground": "#c3c3c3",
    "peekViewEditor.background": "#2a2a2a",
    "dropdown.background": "#2a2a2a",
    "dropdown.border": "#3f3f3f",
    "sideBar.background": "#202020",
    "sideBar.border": "#2f2f2f",
    "sideBarSectionHeader.background": "#343434",
    "activityBar.background": "#202020",
    "activityBar.foreground": "#c3c3c3",
    "activityBar.border": "#2f2f2f",
    "activityBar.activeBorder": "#2eaadc",
    "activityBarBadge.background": "#2eaadc",
    "titleBar.activeBackground": "#2a2a2a",
    "titleBar.inactiveBackground": "#2a2a2a",
    "editor.selectionBackground": "#1cc4ff60",
    "editor.selectionHighlightBackground": "#0fb7fa3a",
    "editor.wordHighlightBackground": "#ffffff00",
    "editor.wordHighlightBorder": "#1cc4ffff",
    "editor.wordHighlightStrongBackground": "#ffffff00",
    "editor.wordHighlightStrongBorder": "#1cc4ffff",
    "editor.hoverHighlightBackground": "#494949",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.lineHighlightBorder": "#2a2a2a",
    "editorWhitespace.foreground": "#494949",
    "editorRuler.foreground": "#494949",
    "editorIndentGuide.background": "#3f3f3f",
    "editorIndentGuide.activeBackground": "#5d5d5d",
    "tree.indentGuidesStroke": "#535353",
    "tree.tableColumnsBorder": "#535353",
    "editorCursor.foreground": "#99ffff",
    "terminalCursor.foreground": "#99ffff",
    "editorLineNumber.foreground": "#535353",
    "editorLineNumber.activeForeground": "#c3c3c3",
    "editorWidget.background": "#2a2a2a",
    "editorSuggestWidget.highlightForeground": "#57d3ff",
    "editorSuggestWidget.selectedBackground": "#343434",
    "tab.activeBorderTop": "#2eaadc",
    "tab.activeBackground": "#2f2f2f",
    "tab.inactiveBackground": "#202020",
    "tab.border": "#2f2f2f",
    "editorGroupHeader.tabsBackground": "#202020",
    "editorGroupHeader.border": "#2a2a2a",
    "editorGroupHeader.tabsBorder": "#2f2f2f",
    "list.activeSelectionBackground": "#343434",
    "list.activeSelectionForeground": "#6be7ff",
    "list.inactiveSelectionBackground": "#2f2f2f",
    "list.inactiveSelectionForeground": "#6be7ff",
    "list.hoverBackground": "#343434",
    "list.focusBackground": "#343434",
    "gitDecoration.modifiedResourceForeground": "#26B5CE",
    "gitDecoration.deletedResourceForeground": "#eb5757",
    "gitDecoration.untrackedResourceForeground": "#4CB782",
    "gitDecoration.ignoredResourceForeground": "#5d5d5d",
    "gitDecoration.conflictingResourceForeground": "#F2994A",
    "diffEditor.insertedTextBackground": "#85c75e26",
    "diffEditor.removedTextBackground": "#e6292926",
    "panel.border": "#2f2f2f",
    "panelTitle.activeBorder": "#2eaadc",
    "badge.background": "#2eaadc",
    "statusBar.foreground": "#c3c3c3"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8d847b"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "entity.name.variable",
        "string constant.other.placeholder",
        "source.vue meta.object.member.ts meta.object.member.ts"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "constant.other.class",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#dde8eb"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#eb5757"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "meta storage.type"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Keyword, Operator",
      "scope": [
        "keyword.definition"
      ],
      "settings": {
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Comparison",
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword.operator.logical.soil"
      ],
      "settings": {
        "foreground": "unset"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.brace",
        "punctuation.support.type.property-name",
        "expression.embbeded punctuation.definition.tag.begin.html",
        "expression.embbeded punctuation.definition.tag.end.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a8a8a8"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation.definition.binding-pattern.object.js"
      ],
      "settings": {
        "foreground": "#a8a8a8"
      }
    },
    {
      "name": "Tag Wrappers",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#74829e"
      }
    },
    {
      "name": "HTML Text",
      "scope": [
        "text.html",
        "meta.jsx.children.js"
      ],
      "settings": {
        "foreground": "#bdc4cc"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "entity.name.type.namespace",
        "keyword.other",
        "meta.directive support.function"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control.flow",
        "keyword.control.export",
        "keyword.control.return",
        "meta.function.closure",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.other.namespace"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#e08594"
      }
    },
    {
      "name": "Function and Method Call",
      "scope": [
        "meta.method.declaration meta.function-call entity.name.function",
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function",
        "variable.function",
        "support.function",
        "entity.name.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#49bde0"
      }
    },
    {
      "name": "Function and Method Definition",
      "scope": [
        "meta.function entity.name.function",
        "meta.method entity.name.method",
        "meta.method.declaration entity.name.function",
        "meta.definition.method entity.name.function"
      ],
      "settings": {
        "foreground": "#49bde0"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#ecbc55"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ec9459"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#bde052"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "string punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#bde05280"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "entity.name.type.class",
        "support.type",
        "support.class",
        "new.expr meta.function-call entity.name.function",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "support.constant",
        "entity.name.type.namespace",
        "meta.tag.custom entity.name.tag.html",
        "source.vue invalid.illegal.unrecognized-tag.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ecbc55"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type",
        "meta.return.type",
        "entity.name.type",
        "meta.use.php"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ecbc55"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "meta.type.tuple meta.brace"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Namespaces and Classes",
      "scope": [
        "entity.other.inherited-class",
        "support.orther.namespace.use",
        "support.other.namespace"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9ebecc"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language",
        "variable.language.this",
        "meta variable.language.this"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#ffffffae"
      }
    },
    {
      "name": "Attributes, HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name",
        "meta.directive.vue keyword.control",
        "meta.directive entity.name.type"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "CSS Classes, IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.name.tag.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a38ec3"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#a2d484"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#eb5757"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#26B5CE"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "@include",
      "scope": [
        "keyword.control.at-rule"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#49bde0"
      }
    },
    {
      "name": "&",
      "scope": [
        "entity.name.tag.reference"
      ],
      "settings": {
        "foreground": "#5fdfff"
      }
    },
    {
      "name": "Property names",
      "scope": [
        "support.type.property-name.json",
        "meta.objectliteral",
        "source.vue meta.object.member.ts meta.object.member.ts meta.object.member.ts",
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#b9b9b9"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#d3782e"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "source.env"
      ],
      "settings": {
        "foreground": "#bde052"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "text.html.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#ecbc55"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#bde052"
      }
    }
  ]
}